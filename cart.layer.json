{
	"activelink":true,
	"tpl":"-cart/cart.tpl",
	"tplroot":"CART",
	"showanimatefastreparse":true,
	"json":"-cart/?type=cart",
	"global":["user", "cart"],
	"conf":"cart",
	"childs":{
		"orders":{
			"tpl":"-cart/cart.tpl",
			"tplroot":"ORDERS",
			"json":"-cart/?type=orders",
			"global":["user", "cart"],
			"child":{
				"name":"order",
				"jsontpl":"-cart/?type=order&id={crumb.name=:my|crumb.name}&place=orders",
				"autosavenametpl":"orders.{crumb.name}",
				"tpl":"-cart/cart.tpl",
				"global":["user", "cart"],
				"tplroot":"ORDER",
				"childs":{
					"sbrfpay":{
						"external":"-cart/sbrfpay/layer.json"
					},
					"paykeeper":{
						"external":"-cart/paykeeper/layer.json"
					},
					"list":{					
						"tpl":"-cart/cart.tpl",
						"tplroot":"LIST",
						"global":["user", "cart"],
						"jsontpl":"-cart/?type=order&id={crumb.parent.name}&place=orders",
						"*jsontpl":"-cart/?type=list&id={crumb.parent.name}&add={crumb.child.child.name}",
						"autosavenametpl":"orders.{crumb.parent.name}"
					},
					"print":{
						"jsontpl":"-cart/?type=order&id={crumb.parent.name}",
						"tpl":"-cart/cart.tpl",
						"global":["user", "cart"],
						"tplroot":"PRINT",
						"dataroot":"data.order"
					}
				}
			}
		},
		"admin":{
			"tpl":"-cart/cart.tpl",
			"jsontpl":"-cart/?type=admin&all={crumb.child.name=:all?:1}",
			"tplroot":"ADMIN",
			"global":["user", "cart"],
			"child":{
				"istpl":"{crumb.name!:all?:yes}",
				"jsontpl":"-cart/?type=order&id={crumb.name}&place=admin",
				"autosavenametpl":"admin.{crumb.name}",
				"tpl":"-cart/cart.tpl",
				"global":["user", "cart"],
				"tplroot":"ADMORDER",
				"childs":{
					"sbrfpay":{
						"external":"-cart/sbrfpay/layer.json"
					},
					"print":{
						"jsontpl":"-cart/?type=order&id={crumb.parent.name}",
						"tpl":"-cart/cart.tpl",
						"global":["user", "cart"],
						"tplroot":"PRINT",
						"dataroot":"data.order"
					},
					"list":{
						"tpl":"-cart/cart.tpl",
						"tplroot":"LIST",
						"global":["user", "cart"],
						"jsontpl":"-cart/?type=order&id={crumb.parent.name}&place=admin",
						"*jsontpl":"-cart/?type=list&id={crumb.parent.name}",
						"autosavenametpl":"admin.{crumb.parent.name}"
					}
				}
			}
		}
	}
}