{
	"domready":true,
	"activelink":true,
	"tpl":"-cart/cart.tpl",
	"tplroot":"OFFICE",
	"json":"-cart/?type=office",
	"global":["sign", "cat_basket"],
	"onsubmit":true,
	"childs":{
		"order":{
			"tpl":"-cart/cart.tpl",
			"tplroot":"ORDER",
			"css":"-cart/catalog.css",
			"global":["sign", "cat_basket"],
			"json":"-cart/order.php",
			"onsubmit":true,
			"divs":{
				"ORDERCART":{
					"sub":true,
					"css":"-cart/catalog.css",
					"json":"-cart/order.php",
					"global":["sign", "cat_basket"],
					"autosavename":"user"
				}
			}
		},
		"orders":{
			"tpl":"-cart/orders.tpl",
			"json":"-cart/orders.php",
			"global":["order", "cat_basket"],
			"child":{
				"jsontpl":"-cart/order.php?id={state.name=:my|state.name}&place=orders",
				"unick":"order",
				"autosavenametpl":"{state.name=:my?:user?:userelse}{userelse:}orders{state.name}",
				"tpl":"-cart/orders.tpl",
				"global":["order", "cat_basket"],
				"tplroot":"orderPage",
				"onsubmit":true,
				"childs":{
					"paycard":{
						"jsontpl":"-cart/paycard.php?id={state.parent.name}&action=paycard",
						"tplroot":"paycard",
						"tpl":"-cart/paycard.tpl",
						"global":["cat_basket","order"]
					},
					"refunds":{
						"jsontpl":"-cart/paycard.php?id={state.parent.name}&action=refunds",
						"tplroot":"refunds",
						"tpl":"-cart/paycard.tpl",
						"global":["cat_basket","order"]
					}
				}
			}
		},
		"admin":{
			"tpl":"-cart/orders.tpl",
			"json":"-cart/orderscontrol.php",
			"tplroot":"adm_root",
			"global":"order",
			"child":{
				"jsontpl":"-cart/order.php?id={state.name}&place=admin",
				"autosavenametpl":"admin{state.name}",
				"tpl":"-cart/orders.tpl",
				"global":"order",
				"onsubmit":true,
				"tplroot":"adm_orderPage"
			}
		},
		"wholesale":{
			"tpl":"-cart/wholesalecontrol.tpl",
			"json":"-cart/wholesalecontrol.php",
			"global":["order", "cat_basket"],
			"onsubmit":true,
			"autosavename":"wholesale",
			"divs":{
				"level":{
					"sub":true,
					"tpl":"-cart/wholesalecontrol.tpl",
					"json":"-cart/wholesalecontrol.php",
					"global":["order", "cat_basket"],
					"autosavename":"wholesale",
					"tplroot":"level",
					"onsubmit":true
				}
			}
		},
		"signup":{
			"tpl":"-cart/sign.tpl",
			"tplroot":"SIGNUP",
			"json":"-cart/sign.php?type=up",
			"onsubmit":true,
			"global":["cat_basket","sign"],
			"autosavename":"user"
		},
		"signin":{
			"tpl":"-cart/sign.tpl",
			"tplroot":"SIGNIN",
			"json":"-cart/sign.php?type=in",
			"onsubmit":true,
			"global":["cat_basket","sign"],
			"autosavename":"user"
		},
		"resendpass":{
			"tpl":"-cart/sign.tpl",
			"tplroot":"RESENDPASS",
			"json":"-cart/sign.php?type=resendpass",
			"onsubmit":true,
			"global":["cat_basket","sign"],
			"autosavename":"user"
		},
		"signout":{
			"tpl":"-cart/sign.tpl",
			"tplroot":"SIGNOUT",
			"json":"-cart/sign.php?type=out",
			"global":["cat_basket","sign"],
			"autosavename":"user"
		}
	}
}