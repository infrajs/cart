{
	"domready":true,
	"activelink":true,
	"tpl":"-cart/cart.tpl",
	"tplroot":"CART",
	"onsubmit":true,
	"json":"-cart/?type=cart",
	"global":["user", "cart"],
	"conf":"cart",
	"childs":{
		"orders":{
			"tpl":"-cart/cart.tpl",
			"tplroot":"ORDERS",
			"json":"-cart/?type=orders",
			"global":["user", "cart"],
			"child":{
				"id":"order",
				"jsontpl":"-cart/?type=order&id={crumb.name=:my|crumb.name}&place=orders",
				"autosavenametpl":"{crumb.parent.name}.{crumb.name}",
				"tpl":"-cart/cart.tpl",
				"global":["user", "cart"],
				"tplroot":"ORDER",
				"childs":{
					"list":{
						"tpl":"-cart/cart.tpl",
						"tplroot":"LIST",
						"global":["user", "cart"],
						"jsontpl":"-cart/?type=list&id={crumb.parent.name}&add={crumb.child.child.name}",
						"autosavenametpl":"{crumb.parent.parent.name}.{crumb.parent.name}"
					}
				}
			}
		},
		"admin":{
			"tpl":"-cart/cart.tpl",
			"json":"-cart/?type=admin",
			"tplroot":"ADMIN",
			"global":"order",
			"child":{
				"jsontpl":"-cart/?type=order&id={crumb.name}&place=admin",
				"autosavenametpl":"admin.{crumb.name}",
				"tpl":"-cart/cart.tpl",
				"global":"order",
				"tplroot":"ADMORDER",
				"childs":{
					"list":{
						"tpl":"-cart/cart.tpl",
						"tplroot":"LIST",
						"global":["user", "cart"],
						"jsontpl":"-cart/?type=list&id={crumb.parent.name}",
						"autosavenametpl":"{crumb.parent.parent.name}.{crumb.parent.name}"
					}
				}
			}
		}
	}
}