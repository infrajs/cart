{
	"activelink":true,
	"tpl":"-cart/layout.tpl",
	"tplroot":"CART",
	"showanimatefastreparse":true,
	"jsontpl":"-cart/api/cart?lang={Cart.lang()}&token={User.token()}",
	"global":["user", "cart"],
	"conf":"cart",
	"childs":{
		"orders":{
			"tpl":"-cart/layout.tpl",
			"tplroot":"ORDERS",
			"jsontpl":"-cart/api/cart?lang={Cart.lang()}&token={User.token()}",
			"global":["user", "cart"],
			"child":{
				"jsontpl":"-cart/api/order?lang={Cart.lang()}&order_nick={crumb.name}&token={User.token()}",
				"autosavenametpl":"orders.{crumb.name}",
				"tpl":"-cart/layout.tpl",
				"global":["user", "cart-order"],
				"globaljson":["cart"],
				"tplroot":"ORDER",
				"childs":{
					"sbrfpay":{
						"global":["user", "cart-order", "cart", "cart-sum"],
						"external":"-cart/sbrfpay/layer.json"
					},
					"paykeeper":{
						"global":["user", "cart-order", "cart", "cart-sum"],
						"external":"-cart/paykeeper/layer.json"
					},
					"list":{
						"tpl":"-cart/layout.tpl",
						"tplroot":"LIST",
						"global":["user","cart-list"],
						"globaljson":["cart", "cart-sum"],
						"jsontpl":"-cart/api/order?lang={Cart.lang()}&order_nick={crumb.parent.name}&token={User.token()}"
					},
					"print":{
						"jsontpl":"-cart/api/order?lang={Cart.lang()}&order_nick={crumb.parent.name}&token={User.token()}",
						"tpl":"-cart/layout.tpl",
						"global":["user", "cart"],
						"tplroot":"PRINT",
						"dataroot":"data.order"
					}
				}
			}
		},
		"admin":{
			"layers":{
				"tpl":"-cart/layout.tpl",
				"tplroot":"ADMIN",
				"divs":{
					"YEARS":{
						"activelink":true,
						"tpl":"-cart/layout.tpl",
						"tplroot":"YEARS",
						"jsontpl":"-cart/api/years?lang={Cart.lang()}&token={User.token()}"
					},
					"ADMINLIST":{
						"tplroot":"ADMINLIST",
						"tpl":"-cart/layout.tpl",
						"global":["user", "cart"],
						"jsontpl":"-cart/api/orders?lang={Cart.lang()}&token={User.token()}&start={Crumb.get.start}"
					}
				}	
			},
			"child":{
				"istpl":"{crumb.name!:all?:yes}",
				"jsontpl":"-cart/api/order?lang={Cart.lang()}&order_nick={crumb.name}&order_nick={crumb.name}&token={User.token()}",
				"autosavenametpl":"admin.{crumb.name}",
				"tpl":"-cart/layout.tpl",
				"global":["user", "cart"],
				"tplroot":"ORDER",
				"childs":{
					"print":{
						"jsontpl":"-cart/api/order?lang={Cart.lang()}&order_nick={crumb.parent.name}&token={User.token()}",
						"tpl":"-cart/layout.tpl",
						"global":["user", "cart"],
						"tplroot":"PRINT",
						"dataroot":"data.order"
					},
					"list":{
						"tpl":"-cart/layout.tpl",
						"tplroot":"LIST",
						"global":["user","cart-list"],
						"globaljson":["cart"],
						"jsontpl":"-cart/api/order?lang={Cart.lang()}&order_nick={crumb.parent.name}&token={User.token()}"
					}
				}
			},
			"childs":{
				"wait":{
					"tpl":"-cart/layout.tpl",
					"tplroot":"ADMIN",
					"divs":{
						"YEARS":{
							"activelink":true,
							"tpl":"-cart/layout.tpl",
							"tplroot":"YEARS",
							"jsontpl":"-cart/api/years?lang={Cart.lang()}&token={User.token()}"
						},
						"ADMINLIST":{
							"tplroot":"ADMINLIST",
							"tpl":"-cart/layout.tpl",
							"global":["user", "cart"],
							"jsontpl":"-cart/api/orders?lang={Cart.lang()}&wait=1&token={User.token()}&start={Crumb.get.start}"
						}
					}
					
				}
			}
		}
	}
}