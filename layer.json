{
	"activelink":true,
	"tpl":"-cart/layout.tpl",
	"tplroot":"CART",
	"showanimatefastreparse":true,
	"jsontpl":"-cart/api/cart?lang={Cart.lang()}&token={User.token()}",
	"global":["user", "cart"],
	"conf":"cart",
	"childs":{
		"orders":{
			"tpl":"-cart/layout.tpl",
			"tplroot":"ORDERS",
			"jsontpl":"-cart/api/cart?lang={Cart.lang()}&token={User.token()}",
			"global":["user", "cart"],
			"child":{
				"name":"order",
				"jsontpl":"-cart/api/order?lang={Cart.lang()}&order_nick={crumb.name}&order_nick={crumb.name}&token={User.token()}",
				"autosavenametpl":"orders.{crumb.name}",
				"tpl":"-cart/layout.tpl",
				"global":["user", "cart-order"],
				"globaljson":["cart"],
				"tplroot":"ORDER",
				"childs":{
					"sbrfpay":{
						"external":"-cart/sbrfpay/layer.json"
					},
					"paykeeper":{
						"external":"-cart/paykeeper/layer.json"
					},
					"list":{					
						"tpl":"-cart/layout.tpl",
						"tplroot":"LIST",
						"global":["user", "cart-list"],
						"globaljson":["cart"],
						"jsontpl":"-cart/api/order?lang={Cart.lang()}&order_nick={crumb.parent.name}&token={User.token()}"
					},
					"print":{
						"jsontpl":"-cart/api/order?lang={Cart.lang()}&order_nick={crumb.parent.name}&token={User.token()}",
						"tpl":"-cart/layout.tpl",
						"global":["user", "cart"],
						"tplroot":"PRINT",
						"dataroot":"data.order"
					}
				}
			}
		},
		"admin":{
			"tpl":"-cart/layout.tpl",
			"jsontpl":"-cart/?type=admin&all={crumb.child.name=:all?:1}",
			"tplroot":"ADMIN",
			"global":["user", "cart"],
			"child":{
				"istpl":"{crumb.name!:all?:yes}",
				"jsontpl":"-cart/api/order?lang={Cart.lang()}&order_nick={crumb.name}&order_nick={crumb.name}&token={User.token()}",
				"autosavenametpl":"admin.{crumb.name}",
				"tpl":"-cart/layout.tpl",
				"global":["user", "cart"],
				"tplroot":"ORDER",
				"childs":{
					"sbrfpay":{
						"external":"-cart/sbrfpay/layer.json"
					},
					"print":{
						"jsontpl":"-cart/api/order?lang={Cart.lang()}&order_nick={crumb.parent.name}&token={User.token()}",
						"tpl":"-cart/layout.tpl",
						"global":["user", "cart"],
						"tplroot":"PRINT",
						"dataroot":"data.order"
					},
					"list":{
						"tpl":"-cart/layout.tpl",
						"tplroot":"LIST",
						"global":["user", "cart"],
						"jsontpl":"-cart/api/order?lang={Cart.lang()}&order_nick={crumb.parent.name}&token={User.token()}",
						"autosavenametpl":"admin.{crumb.parent.name}"
					}
				}
			}
		}
	}
}